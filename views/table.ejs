     <title>Coronavirus Outbreak in India</title>
    <base href="/"> 
    <script type="text/javascript" src="../public/datatable/js/jquery-3.5.1.js"></script>
    <script type="text/javascript" src="../public/datatable/js/jquery.dataTables.min.js"></script>
    <script type="text/javascript" src="../public/datatable/js/dataTables.responsive.min.js"></script>

    <script type="text/javascript" src="../public/datatable/js/datatable.js"></script>

    <link rel="stylesheet" href="../public/datatable/css/jquery.dataTables.min.css">
    <link rel="stylesheet" href="../public/datatable/css/responsive.dataTables.min.css">

</head>
<body>
    <div class="container-fluid mb-2" style="background-color: #eee;">
        <div class="row">
            <div class="col-12 col-md-6">
              <h1 class="font-weight-bold">India</h1>
              <p class="font-weight-normal"><%= "Last Updated on " + new Date()%></p>
          </div>
          <div  class="col-12 col-md-6 ">
            <h1> <%=data[0].Active %></h1>
            <p class="font-weight-bold">Active</p>
          </div>
      
            
          </div>
    </div>


    <div class="container">
    <table id="myTable" class="display responsive nowrap" style="width:100%">
        <thead>
            <tr>
                <th >#</th>
                <th >Sate</th>
                <th >Confirmed</th>
                <th >Active</th>
                <th >Recovered</th>
                <th >Death</th>
              </tr>
        </thead>
        <tbody>
            <% data.forEach((element,index) => {%>
                <tr>
                    <td><%=index%></td>
                    <td>  <%=element.State%> </td>
                    <td>  <%=element.Confirmed%></td>
                    <td><%=element.Active%></td>
                    <td><%=element.Recovered%></td>
                    <td><%=element.Death%></td>  
                </tr>
                <% }); %>
        </tbody>
    </table>
</div>

</div>
<div class="d-flex justify-content-around">
