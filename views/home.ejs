<!doctype html>
<html lang="en">
  <head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.0/css/bootstrap.min.css" integrity="sha384-9aIt2nRpC12Uk9gS9baDl411NQApFmC26EwAOH8WgZl5MYYxFfc+NcPb1dKGj7Sk" crossorigin="anonymous">

    <title>Coronavirus Outbreak in India</title>
  </head>
  <body>
    <div class="jumbotron">
      <div class="container">
          <div class="d-flex justify-content-between">
            <div>
                <h1 class="font-weight-bold">India</h1>
                <p class="font-weight-normal"><%= "Last Updated on " + new Date()%></p>
            </div>
            <div>
               
             
                <h1> <%=data[0].Active %></h1>
                <p class="font-weight-bold">Active</p>

            </div>
          </div>
       
      </div>
    </div>
    <div class="container">
    <!-- thead-light -->
    <table class="table">
        <thead class="thead-dark">
          <tr>
            <th scope="col">#</th>
            <th scope="col">Sate</th>
            <th scope="col">Confirmed</th>
            <th scope="col">Active</th>
            <th scope="col">Recovered</th>
            <th scope="col">Death</th>
          </tr>
        </thead>
        <tbody>
          <!-- <tr>
            <th scope="row">1</th>
            <td>Mark</td>
            <td>Otto</td>
            <td>@mdo</td>
          </tr> -->
          <% data.forEach((element,index) => {%>
            <tr>
                <td><%=index%></td>
                <td>  <%=element.State%> </td>
                <td>  <%=element.Confirmed%></td>
                <td><%=element.Active%></td>
                <td><%=element.Recovered%></td>
                <td><%=element.Death%></td>  
            </tr>
            <% }); %>
        </tbody>
      </table>
      <div class="d-flex justify-content-between">
        <h6><a href="/">HOME</a></h6> 
         <h6><a href="https://www.covid19india.org/">Covid India</a></h6>
     
     </div>
    </div>

    <section  >

        <div class="col-lg-12 col-sm-12 bottom-container text-center pt-3" style="background-color: #dde6e6; padding: 7px;">
          <a class="footer-link " href="https://www.facebook.com" target="_blank">
            <img class="profile" src="images/facebook32.png" alt="no image found">
          </a>
          <a class="footer-link" href="https://www.linkedin.com/in/hasnainshaikh312/" target="_blank">
            <img class="profile" src="images/linkedin32.png" alt="no image found">
          </a>
          <a class="footer-link" href="https://www.instagram.com/godelementrix/?r=nametag" target="_blank">
            <img class="profile" src="images/instagram-sketched32.png" alt="no image found">
          </a>
          <a class="footer-link" href="https://mail.google.com/mail/u/0/?view=cm&amp;fs=1&amp;tf=1&amp;source=mailto&amp;su=Testing+out+mailto!&amp;body=name%3D%0D%0Aemail%3D%0D%0Aphone%3D%0D%0Amessage%3D%0D%0A%0D%0A&amp;to=hasnains312@gmail.com" target="_blank">
            <img class="profile" src="images/email.png" alt="no image found">
          </a>
    
          <p class="copyright pt-2 ">Â© 2021 Copyright: Hasnain Shaikh-hasnain393/GitHub. </p>
      
        </div>
     
      </section>
   
    
     
    <!-- Optional JavaScript -->
    <!-- jQuery first, then Popper.js, then Bootstrap JS -->
